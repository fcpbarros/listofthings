<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <ListView
            android:id="@+id/lista_transacoes_listview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:divider="@null"
            android:dividerHeight="0dp" />

    </LinearLayout>

    <com.github.clans.fab.FloatingActionMenu
        android:id="@+id/lista_transacoes_adiciona_menu"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:paddingBottom="10dp"
        fab:menu_backgroundColor="#ccffff"
        fab:menu_colorNormal="#02508E"
        fab:menu_colorPressed="#02508E"
        fab:menu_colorRipple="#02508E"
        fab:menu_labels_colorNormal="#fff"
        fab:menu_labels_ellipsize="end"
        fab:menu_labels_singleLine="true"
        fab:menu_labels_textColor="#fff">


        <com.github.clans.fab.FloatingActionButton
            android:id="@+id/lista_transacoes_adiciona_veiculo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src=""
            fab:fab_colorNormal=""
            fab:fab_colorPressed="#2945fc"
            fab:fab_colorRipple="#3216e2"
            fab:fab_label=""
            fab:fab_size="mini" />


    </com.github.clans.fab.FloatingActionMenu>

</RelativeLayout>